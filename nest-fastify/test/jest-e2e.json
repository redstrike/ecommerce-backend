{
	"moduleFileExtensions": [
		"js",
		"json",
		"ts"
	],
	"rootDir": ".",
	"testEnvironment": "node",
	"testRegex": ".e2e-spec.ts$",
	"transform": {
		"^.+\\.(t|j)s$": [
			"ts-jest",
			{
				"tsconfig": "./test/tsconfig.e2e.json"
			}
		]
	},
	"moduleNameMapper": {
		"^@paralleldrive/cuid2$": "<rootDir>/__mocks__/@paralleldrive/cuid2.js"
	}
}